{
	"name": "@terraforge/terraform",
	"version": "0.0.9",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/terraforge-js/terraforge.git"
	},
	"bugs": {
		"url": "https://github.com/terraforge-js/terraforge/issues"
	},
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"bin": {
		"build-package": "./cli/build-package.ts"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsup src/index.ts --format esm --dts --clean --out-dir ./dist",
		"prepublishOnly": "bun run build",
		"test": "bun test"
	},
	"peerDependencies": {
		"@terraforge/core": "workspace:*"
	},
	"dependencies": {
		"@grpc/grpc-js": "1.12.6",
		"@grpc/proto-loader": "^0.7.13",
		"change-case": "^5.4.4",
		"jszip": "^3.10.1",
		"msgpackr": "^1.11.0",
		"semver": "^7.7.1"
	},
	"devDependencies": {
		"@types/semver": "^7.7.0"
	},
	"files": [
		"dist"
	],
	"license": "MIT"
}
