{
	"name": "@terraforge/aws",
	"version": "6.26.0",
	"type": "module",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"typegen": "bun ../terraform/cli/build-package --org hashicorp --type aws",
		"build": "bunup src/index.ts --format esm --dts --clean --target node --packages external --out-dir ./dist",
		"prepublishOnly": "bun run typegen; bun run build"
	},
	"peerDependencies": {
		"@terraforge/terraform": "workspace:*",
		"@terraforge/core": "workspace:*"
	},
	"files": [
		"dist"
	],
	"license": "MIT"
}
